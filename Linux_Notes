create directory 
  mkdir
  
create file
  touch
                                    
                    
delete
  rm 
  rmdir
  
  rm -r             -r stands for recursive (all inside the folder)
  rm -r delfolder/
  rm -ri               would ask for every file
                    
                    
                    
